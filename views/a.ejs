<html>
<head> 
<script type="text/javascript" src="https://canvasjs.com/assets/script/jquery-1.11.1.min.js"></script>
<script type="text/javascript" src="https://canvasjs.com/assets/script/canvasjs.min.js"></script> 
<script type="text/javascript">
	var h=<%=h%>;
	var c=<%=c%>;
	var t=Math.sqrt(2*h/9.8);
	var dps = [
				{ x:0, y: h },
				{ x:t, y:0},
			];
		var flag=true;
		var i=1;
		var xVal=t,xV=0, yVal=0,count1=1,count2=1,bounce=1;
		while(flag){	
		if(i%2!=0){
			yVal = h*Math.pow(c,count1);
			xV=t*Math.pow(c, count1);
			xVal=xVal+xV;
			count1=count1+1;
			dps.push({x: xVal,y: yVal});
			if(yVal<=0.1){				
				flag=false;
				break;
			}
		}
		else
		{
			yVal = 0;
			xV=2*t*Math.pow(c, count2);
			xVal=xVal+xV;
			count2=count2+1;
			bounce=bounce+1;
			dps.push({x: xVal,y: yVal});
		}
		i=i+1;
		}
		
	window.onload = function () {
		
	var chart = new CanvasJS.Chart("chartContainer", { 
		title: {
			text: "Graph:"
		},
		axisX: {						
			title: "Time"
		},
		axisY: {						
			title: "Height"
		},
		data: [
		{
			type: "spline",
			dataPoints:dps,
		}
		]
	});
	
	chart.render();	
}
	document.write("Number of Bounce:");
	document.write(bounce);
</script>
</head>  
<body>  
<div id="chartContainer" style="width:100%; height:280px"></div>  
</body>
</html>